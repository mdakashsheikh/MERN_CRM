{
    "env": {
        "node": true,
        "es2022": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "process": "readonly",
        "androidVersion": "readonly",
        "iOSVersion": "readonly",
        "tizenVersion": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-extra-semi": "error",
        "no-invalid-regexp": "error"
    }
}